@page "/home"
@layout MainLayout

<div class="main-content">
        <div>
            <h1 class="sub-headding">Resources</h1>

            <div class="resource-buttongroup-container">
                <Navigation_Icon_button Button_Title="Supervised" Button_Icon="bi bi-person-check" Button_Icon_Color="primary-color" />
                <Navigation_Icon_button Button_Title="Unsupervised" Button_Icon="bi bi-person-x" Button_Icon_Color="primary-color" />
                <Navigation_Icon_button Button_Title="Regression" Button_Icon="bi bi-bar-chart-line" Button_Icon_Color="primary-color" />
                <Navigation_Icon_button Button_Title="Classification" Button_Icon="bi bi-collection" Button_Icon_Color="primary-color" />
            </div>

            <div class="resource-buttongroup-drop-down-menu">
                <DropDownMenu_Card CardBuildData=@ExampleCard>  </DropDownMenu_Card>
            </div>
        </div>

        
    

        <div>
            <h1 class="sub-headding">Documentation</h1>
            <div class="documentation-group-container">
            <Documentation_Card Card_Title="Supervised" Card_Body_Text=@Supervised_Learning_Card_Content> </Documentation_Card>
            <Documentation_Card Card_Title="Unsupervised" Card_Body_Text=@Unsupervised_Learning_Card_Content />
            <Documentation_Card Card_Title="Regression" Card_Body_Text=@Regression_Card_Content />
            <Documentation_Card Card_Title="Classification" Card_Body_Text=@Classification_Learning_Card_Content />
            </div>
        </div>

        

</div>



